{
  "name": "package-json-to-readme",
  "version": "1.5.1",
  "description": "Generate a README.md from package.json contents",
  "main": "index.js",
  "scripts": {
    "test": "mocha && mos test -x template.md",
    "md": "mos -x template.md"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/zeke/package-json-to-readme"
  },
  "keywords": [
    "package.json",
    "readme",
    "generator",
    "markdown",
    "documentation"
  ],
  "author": "Zeke Sikelianos <zeke@sikelianos.com> (http://zeke.sikelianos.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeke/package-json-to-readme/issues"
  },
  "homepage": "https://github.com/zeke/package-json-to-readme",
  "dependencies": {
    "github-url-to-object": "^1.6.0",
    "hogan.js": "^2.0.0",
    "strip-ansi": "^0.2.2",
    "sync-exec": "^0.5.0",
    "yargs": "^3.0.4"
  },
  "bin": {
    "package-json-to-readme": "index.js",
    "readme": "index.js"
  },
  "preferGlobal": true,
  "devDependencies": {
    "mocha": "^2.1.0",
    "mos": "^1.3.1",
    "mos-plugin-readme": "^1.0.4",
    "nixt": "^0.3.0"
  },
  "mos": {
    "plugins": [
      "readme"
    ],
    "installation": {
      "useShortAlias": true
    }
  }
}
